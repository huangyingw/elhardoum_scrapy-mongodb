[remote "elhardoum"]
    url = https://github.com/elhardoum/scrapy-mongodb.git
    fetch = +refs/heads/*:refs/remotes/elhardoum/*
[remote "origin"]
    url = git@github.com:huangyingw/elhardoum_scrapy-mongodb.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = elhardoum
    target = elhardoum/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/elhardoum/scrapy-mongodb
